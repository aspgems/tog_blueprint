</head>

<body>
  <div id="container" class="container">
    <div id="header" class="span-24 last">
      <div id="header_content" class="span-24 last">
        <div class="span-5">
          <h1 id="logo"><a href="/"><img src="/tog_blueprint/images/template/logo.gif" alt="<%=config['plugins.tog_core.site.name']%>" /></a><br />Administration site</h1>
        </div>

        <div class="span-19 last">
          <div id="header_options">
            <%= render :partial => "shared/header_options" %>
          </div> <!-- /#header_options -->
        </div>

        <div class="span-24 last">&nbsp;</div>

        <div id="menu" class="span-17">
          <ul>
            <%= links_for_navigation :admin %>
          </ul>
        </div>  <!-- #menu -->

        <div class="span-7 last">
          <div id="header_search">
            <%= render :partial => "shared/search_form" %>
          </div> <!-- /#header_search -->
        </div>

      </div> <!-- /#header_content -->
    </div>  <!-- /#header -->

    <div id="pagebody" class="span-24 last">

      <div class="span-24 last">
        <%= render_flash_messages %>
      </div>

      <div class="<%= columns_distribution %> span-24 last">
        <div class="span-14">
          <%= yield %>
        </div> <!-- /.left_col -->

        <div class="span-10 last">
          <%= yield :sidebar %>
        </div> <!-- /.right_col -->
      </div> <!-- /.col_60_40 -->
    
    </div><!-- /#pagebody -->

    <div class="span-24 last">
      <%= render :partial => "shared/footer" %>
    </div>

  </div> <!-- #container -->
</body>
</html>
